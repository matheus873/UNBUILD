    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/pluginutils": "^4.2.0",
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "defu": "^5.0.1",
    "esbuild": "^0.14.25",
    "hookable": "^5.1.1",
    "jiti": "^1.13.0",
    "magic-string": "^0.26.1",
    "mkdirp": "^1.0.4",
    "mkdist": "^0.3.10",
    "mlly": "^0.4.3",
    "mri": "^1.2.0",
    "pathe": "^0.2.0",
    "pkg-types": "^0.3.2",
    "pretty-bytes": "^6.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.0",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "scule": "^0.2.1",
    "typescript": "^4.6.2",
    "untyped": "^0.4.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/chai": "latest",
    "@types/mkdirp": "latest",
    "@types/mocha": "latest",
    "@types/mri": "latest",
    "@types/node": "latest",
    "@types/rimraf": "latest",
    "chai": "latest",
    "eslint": "latest",
    "mocha": "latest",
    "standard-version": "latest"
  }
